<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.test.reference.props'))" />
  <PropertyGroup>
    <PackageId>Search.Tests</PackageId>
    <Description>Search.Tests Class Library</Description>
    <AssemblyName>Search.Tests</AssemblyName>
    <VersionPrefix>1.0.0-preview</VersionPrefix>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>netcoreapp1.1</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="1.0.2" />
    <PackageReference Include="Microsoft.Azure.Search" Version="4.0.0-preview" />
    
    <!--<ProjectReference Include="..\Microsoft.Azure.Search\Microsoft.Azure.Search.csproj" />-->
  </ItemGroup>
  
  <ItemGroup>
    <None Update="SessionRecords\**\*.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <!--Do not remove until VS Test Tools fixes #472-->
  <ItemGroup>
    <Service Include="{82a7f48d-3b50-4b1e-b82e-3ada8210c358}" />
  </ItemGroup>

</Project>
